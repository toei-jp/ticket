{
  "name": "ticket",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-client": "ng serve --environment=local",
    "build-client": "npm run build-client-development && npm run build-client-test && npm run build-client-production",
    "build-client-local": "ng build --environment=local --output-path=./dist/client/development --aot",
    "build-client-development": "ng build --environment=dev --output-path=./dist/client/development --aot",
    "build-client-test": "ng build --environment=test --output-path=./dist/client/test --aot",
    "build-client-production": "ng build --environment=prod --output-path=./dist/client/production --aot",
    "test-client": "ng test --code-coverage",
    "lint-client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
    "e2e": "ng e2e",
    "css-server": "npm run scss-server && postcss dist/server/public/css/**/*.css -u autoprefixer -d dist/server/public/css",
    "scss-server": "node-sass src/server/public/scss --output dist/server/public/css",
    "start": "npm run start-server",
    "ssl": "node dist/server/ssl",
    "start-server-ssl": "node dist/server/ssl",
    "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
    "lint-server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
    "build": "npm run css-server && npm run build-server && npm run build-client",
    "preversion": "npm run build",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "test": "npm -v"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "@toei-jp/cinerino-api-javascript-client": "^1.0.0-alpha.5",
    "@toei-jp/cinerino-api-nodejs-client": "^1.0.0-alpha.7",
    "@types/swiper": "^3.4.5",
    "angular2-moment": "^1.7.1",
    "angular2-useful-swiper": "^5.0.1",
    "aws-sdk": "^2.176.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "http-status": "^1.0.1",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "qrcode": "^1.2.0",
    "redis": "^2.8.0",
    "rxjs": "^5.5.6",
    "swiper": "^3.4.2",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/language-service": "^5.1.3",
    "@types/basic-auth": "^1.1.2",
    "@types/body-parser": "^1.16.8",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/google.analytics": "0.0.36",
    "@types/helmet": "0.0.37",
    "@types/http-status": "^0.2.30",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/moment-timezone": "^0.5.4",
    "@types/node": "^8.5.5",
    "@types/qrcode": "^0.8.0",
    "@types/redis": "^2.8.6",
    "@types/uuid": "^3.4.3",
    "autoprefixer": "^8.2.0",
    "codelyzer": "~4.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "postcss": "^6.0.20",
    "postcss-cli": "^5.0.0",
    "protractor": "^5.4.1",
    "ts-node": "~4.0.2",
    "tslint": "~5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.2"
  }
}
